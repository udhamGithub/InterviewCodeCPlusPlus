#include<iostream>
using namespace std;

class ArrayClass
{
	int *ptr;
	int size;
public:
	ArrayClass(int xArray[],int  size)
	{
		ptr=NULL;
		size=0;
		cout<<"Aconstructyre";
	}

};
int main()
{
	int A[]={1,2,3,4,5};
	ArrayClass  obj(A,5);
}
