#include<iostream>
#include<pthread.h>
using namespace std;
void *thread_1(void *)
{
	//crtical resource
}
void *thread_2(void *)
{
	//crtical resource
}

int main()
{
	pthread_t pid1;
	pthread_t pid2;
	pthread_create(&pid1,NULL,thread_1, NULL);

	pthread_create(&pid1,NULL,thread_1, NULL);
	//pthread_create();

	cout<<"My First Program";
	return 0;
}




