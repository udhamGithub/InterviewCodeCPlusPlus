#include<iostream>
#include<pthread.h>
using namespace std;

void Thread_1()
{
	cout<<"Common resource :Printer";
}
void Thread_2()
{
	//
}

int main()
{
	pthread_t pid1;
	pthread_t pid2;

	pthread_create(&pid1,NULL,Thread_1,NULL);


}




